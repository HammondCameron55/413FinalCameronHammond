@{
}

@model List<Entertainers>

<div class="text-center">
    <h1 class="display-4">Artists</h1>
    <p>Here is where the entertainers will be displayed</p>

</div>

<div class="text-center">
    <h1 class="display-4">Artists</h1>
    <p>Here is where the entertainers will be displayed:</p>
    <ul>
        @foreach (var entertainer in Model)
        {
            <li>@entertainer.EntStageName</li>  <!-- Displaying the stage name of each entertainer -->
        }
    </ul>
</div>


<div>
    <a class="btn btn-primary" asp-action="AddArtist">Add Artist</a>
</div>